{
  "vim_mode": true,
  "vim": {
    "toggle_relative_line_numbers": true,
    "use_smartcase_find": true,
    "use_multiline_find": true
  },
  "ui_font_size": 16,
  "buffer_font_size": 16,
  "theme": {
    "mode": "system",
    "light": "One Light",
    "dark": "Gruvbox Dark Soft"
  },
  "cursor_blink": false,
  "cursor_shape": "block",
  "scrollbar": {
    "show": "never"
  },
  "tab_bar": {
    "show_nav_history_buttons": false
  },
  "tabs": {
    "file_icons": true,
    "git_status": true
  },
  "toolbar": {
    "breadcrumbs": false,
    "quick_actions": false
  },
  "git": {
    "git_gutter": "tracked_files",
    "inline_blame": {
      "enabled": true,
      "show_commit_summary": true
    }
  },
  "inlay_hints": {
    "enabled": true
  },
  "telemetry": {
    "diagnostics": false,
    "metrics": false
  },
  "terminal": {
    "blinking": "off",
    "font_family": "JetBrainsMono Nerd Font",
    "font_size": 16,
    "working_directory": "current_project_directory"
  },
  "project_panel": {
    "button": true,
    "dock": "left"
  },
  "assistant": {
    "enabled": false,
    "button": false,
    "version": "1"
  },
  "show_completions_on_input": true,
  "features": {
    "inline_completion_provider": "supermaven"
  },
  "hover_popover_enabled": false,
  "notification_panel": {
    "button": false
  },
  "chat_panel": { "button": false },
  "collaboration_panel": { "button": false }
}
